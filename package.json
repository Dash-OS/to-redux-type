  "author": "Braden R. Napier",
  "license": "MIT",
  "scripts": {
    "prepublishOnly": "yarn build",
    "build": "rimraf dist && cross-env NODE_ENV=production babel --out-dir dist src",
    "try": "babel-node try-test.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Dash-OS/to-redux-type.git"
  },
  "bugs": {
