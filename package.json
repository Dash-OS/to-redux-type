    "babili-webpack-plugin": "^0.1.1",
    "colors": "^1.1.2",
    "cross-env": "^5.0.0",
    "happypack": "^3.1.0",
    "lodash-webpack-plugin": "^0.11.4",
    "rimraf": "^2.6.1",
    "webpack": "^2.6.1",
